{
  "name": "firebase-admin-server",
  "version": "0.0.0",
  "description": "A Web Application that acts as a wrapper around the Firebase Admin SDK",
  "scripts": {
    "build": "tsc",
    "start": "node ./build/server",
    "start:dev": "cross-env DEBUG=firebase-admin:* ts-node-dev ./src/server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fvilers/firebase-admin-server.git"
  },
  "keywords": [
    "firebase",
    "firebase-admin",
    "firebase-admin-sdk"
  ],
  "author": "Fabian Vilers <fabian.vilers@dev-one.com> (https://www.dev-one.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fvilers/firebase-admin-server/issues"
  },
  "homepage": "https://github.com/fvilers/firebase-admin-server#readme",
  "dependencies": {
    "debug": "^4.3.2",
    "express": "^4.17.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.7.10",
    "cross-env": "^7.0.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.2"
  }
}
